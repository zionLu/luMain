.weui-navbar__slider{
    width: 187.5rpx;
    background-color: #29BB73;
}
.wrapper{
    height: 100%;
}
.page__bd{
    height: 100%;
}
.weui-navbar{
    position: fixed;
}
swiper{
    width: 100%;
    height: 90%;
    position: absolute;
    top: 50px;
}
.image{
    width: 720rpx;
    height: 600rpx;
    margin: 30rpx auto 30rpx;
    border-radius: 20rpx;
    display: block;
}
.info{
    width: 670rpx;
    height: 400rpx;
    margin: 0 auto 20rpx;
    padding: 20rpx;
    border-radius: 20rpx;
    background: #FFFFFF;
    display: flex;
    justify-content: center;
    flex-direction: column;
    font-size: 35rpx;
    color: #888888;
}
.title{
    display: flex;
    justify-content: center;
    color: #353535;
    font-size: 60rpx;
    position: relative;
    top: -20rpx;
}
.intro{
     width: 700rpx;
     margin: 50rpx auto;
     padding: 10rpx;
     border-radius: 20rpx;
     background: #FFFFFF;
     color: #353535;
     font-size: 30rpx;
}
.contents{
     width: 700rpx;
     height: 88%;
     margin: 50rpx auto;
     padding: 10rpx 0 0 10rpx;
     border-radius: 20rpx;
     background: #FFFFFF;
     color: #353535;
     font-size: 30rpx;
     overflow: hidden;
}
.site{
     width: 700rpx;
     margin: 50rpx auto;
     padding: 10rpx;
     border-radius: 20rpx;
     background: #FFFFFF;
     color: #353535;
     font-size: 25rpx;
     display: flex;
     justify-content: center;
     align-items: center;
}
.library{
    flex: 4;
}
.coordinate{
    flex: 3;
    text-align: center;
}
.status{
    flex: 3;
    text-align: right;
}