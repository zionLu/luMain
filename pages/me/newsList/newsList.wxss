.newsList{
    width: 730rpx;
    margin: 15rpx auto;
    padding: 20rpx 0;
    border-radius: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    font-size: 25rpx;
    background: #FFFFFF;
}
.title{
    width: 690rpx;
    color: #576B95;
    padding: 10rpx 20rpx;
    font-size: 30rpx;
    border-bottom: 1px solid #E1E1E1;
    display: flex;
    align-content: flex-start;
    justify-content: flex-start;
    flex-direction: row;
}
.info{
    width: 690rpx;
    padding: 10rpx 20rpx;
    color: #888888;
}
