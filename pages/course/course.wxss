.wrapper{
    height: 100%;
    font-size: 25rpx;
    color: #888888;
}
.course{
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;   
}
.times{
    width: 75rpx;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}
.blank{
    width: 75rpx;
    height: 70rpx;
    background: #FFFFFF;
}
.time{
    width: 1425rpx;
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    border-top: 1rpx solid rgba(0,0,0,0.1);
}
.weekday{
    flex: 1;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    flex-direction: column;
}
.stress{
    font-weight: bold;
    color: #8C8D8F;
}
.select{
    color: #29BB73;
}
.date{
    width: 100%;
    height: 70rpx;
    background: #FFFFFF;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}
.subject{
    width: 90%;
    height: 100%;
    padding: 0 5%;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    color: #FFFFFF;
    font-size: 23rpx;
    font-weight: 600;
}
.index{
    width: 50px;
    height: 50px;
    background: #FF5353;
    color: #FFFFFF;
    font-size: 25px;
    font-weight: bold;
    border-radius: 100%;
    text-align: center;
    line-height: 50px;
    position: fixed;
    bottom: 130rpx;
    right: 70rpx;
    box-shadow: 3px 3px 3px rgba(144,144,144,0.7);
}